make atmega32 AVR_FREQ=4000000L BAUD_RATE=9600 UART=0 LED=B0 LED_START_FLASHES=2
make atmega32 AVR_FREQ=2000000L BAUD_RATE=250000 UART=0 LED=B0 LED_START_FLASHES=2
