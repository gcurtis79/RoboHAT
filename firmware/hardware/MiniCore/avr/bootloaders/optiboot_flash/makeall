make atmega16 AVR_FREQ=4000000L BAUD_RATE=500000 UART=0 LED=B0 LED_START_FLASHES=2
make atmega16 AVR_FREQ=4000000L BAUD_RATE=250000 UART=0 LED=B0 LED_START_FLASHES=2
make atmega16 AVR_FREQ=4000000L BAUD_RATE=19200 UART=0 LED=B0 LED_START_FLASHES=2
