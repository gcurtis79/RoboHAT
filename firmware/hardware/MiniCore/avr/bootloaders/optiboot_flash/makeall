#!/bin/bash
#### Execute  ./makeall to build all variants of this bootloader
#### run $ chmod +x if this file isn't executable.

make clean


# ATmega8/A
make atmega8 AVR_FREQ=20000000L BAUD_RATE=500000 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=20000000L BAUD_RATE=250000 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=20000000L BAUD_RATE=115200 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=18432000L BAUD_RATE=115200 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=16000000L BAUD_RATE=1000000 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=16000000L BAUD_RATE=500000 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=16000000L BAUD_RATE=250000 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=16000000L BAUD_RATE=250000 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=16000000L BAUD_RATE=115200 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=12000000L BAUD_RATE=500000 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=12000000L BAUD_RATE=250000 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=12000000L BAUD_RATE=57600 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=8000000L BAUD_RATE=1000000 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=8000000L BAUD_RATE=500000 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=8000000L BAUD_RATE=250000 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=8000000L BAUD_RATE=115200 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=8000000L BAUD_RATE=57600 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=8000000L BAUD_RATE=38400 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=8000000L BAUD_RATE=19200 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=4000000L BAUD_RATE=500000 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=4000000L BAUD_RATE=250000 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=4000000L BAUD_RATE=19200 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=4000000L BAUD_RATE=9600 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=2000000L BAUD_RATE=250000 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=2000000L BAUD_RATE=19200 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=2000000L BAUD_RATE=9600 UART=0 LED=B5 LED_START_FLASHES=2
make atmega8 AVR_FREQ=1000000L BAUD_RATE=9600 UART=0 LED=B5 LED_START_FLASHES=2

# ATmega16/A
